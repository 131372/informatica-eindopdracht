keys = Array("countU" , "countC", "countD", "countB", "countS", "countAntiU", "countAntiC", "countAntiD", "countAntiB", "countAntiS")


function scoreWaargenomenHadron (cards, keys, waargenomenHadronen){
	$.each(cards, function(keys, value){
		$.each(value, function(keys, value){
			$.each(value, function(keys, value){
				switch(value.name) {
					case'u':
						countU++;
						break;
					case'c':
						countC++;
						break;
					case'd':
						countD++;
						break;	
					case'b':
						countB++;
						break;
					case's':
						countS++;
						break;		
				}
			}
		}
	}
	

}
